	def onInput_onStart(self, p):
		p = self.typeConversion(p)
		if(p == self.typeConversion("non")):
			self.output_1(p)
		elif(p == self.typeConversion("no")):
			self.output_2(p)
		elif(p == self.typeConversion("yes")):
			self.output_3(p)
		elif(p == self.typeConversion("oui")):
			self.output_4(p)
		elif(p == self.typeConversion("si")):
			self.output_5(p)
		else:
			self.onDefault()

	def typeConversion(self, p):
		try:
			p = float(p)
			pint = int(p)
			if( p == pint ):
				p = pint
		except:
			p = str(p)
		return p]]>
