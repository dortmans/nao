                              "exit",
                              "repeat"]
                    for sKind in aKinds:
                        tag = sKind
                        blockWordsForThisKind = blockSpeechReco.getElementsByTagName( sKind )[0]
                        self.aAllWords[sLanguage][sKind] = blockWordsForThisKind.getAttribute( "text" ).encode("utf-8").split("/")
                    # end - variable used for the speech recognition
